//------------------------------------------------
//Código criação EMPREGADO

CREATE TABLE Empregado (
  id INT NOT NULL,
  nome VARCHAR(255) NOT NULL,
  cargo VARCHAR(32) NOT NULL,
  salario DECIMAL(8,2),
  PRIMARY KEY (id)
);

