//------------------------------------------------
//Código criação APLICATIVO

CREATE TABLE Aplicativo
(Id INT NOT NULL, Nome VARCHAR (255),
 Desenvolvedor VARCHAR (255), NumeroDeDownloads INT NOT NULL
 );

